<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<fx:root prefHeight="240.0" prefWidth="300.0" type="AnchorPane" xmlns="http://javafx.com/javafx/17.0.2-ea"
         xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.kizzzy.javafx.setting.SettingGroup">
    <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
               AnchorPane.topAnchor="0.0">
        <children>
            <VBox spacing="8.0" styleClass="group-box">
                <StackPane.margin>
                    <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                </StackPane.margin>
                <children>
                    <AnchorPane VBox.vgrow="ALWAYS">
                        <VBox fx:id="root" spacing="8.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" VBox.vgrow="ALWAYS"/>
                    </AnchorPane>
                </children>
                <padding>
                    <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                </padding>
            </VBox>
            <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" styleClass="group-name"
                        StackPane.alignment="TOP_LEFT">
                <children>
                    <Label fx:id="label" maxHeight="-Infinity" maxWidth="-Infinity" text="LABEL"
                           AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                           AnchorPane.topAnchor="0.0"/>
                </children>
                <padding>
                    <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                </padding>
                <StackPane.margin>
                    <Insets left="16.0"/>
                </StackPane.margin>
            </AnchorPane>
        </children>
    </StackPane>
    <stylesheets>
        <URL value="@/css/global.css"/>
        <URL value="@setting_group_view.css"/>
    </stylesheets>
</fx:root>
