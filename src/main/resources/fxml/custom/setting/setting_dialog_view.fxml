<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<fx:root type="AnchorPane" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">
            <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" prefViewportHeight="300.0"
                        BorderPane.alignment="CENTER" VBox.vgrow="ALWAYS">
                <content>
                    <AnchorPane>
                        <children>
                            <VBox fx:id="root" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                  AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                <padding>
                                    <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                                </padding>
                            </VBox>
                        </children>
                    </AnchorPane>
                </content>
            </ScrollPane>
            <HBox alignment="CENTER_RIGHT" spacing="8.0" VBox.vgrow="NEVER">
                <children>
                    <Button fx:id="btn_cancel" mnemonicParsing="false" text="取消"/>
                    <Button fx:id="btn_save" mnemonicParsing="false" text="保存"/>
                    <Button fx:id="btn_save_exit" layoutX="66.0" layoutY="18.0" mnemonicParsing="false" text="保存并退出"/>
                </children>
                <padding>
                    <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                </padding>
            </HBox>
        </VBox>
    </children>
</fx:root>
