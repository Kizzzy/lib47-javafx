<?xml version="1.0" encoding="UTF-8"?>

<?import cn.kizzzy.javafx.control.LabeledTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import java.net.URL?>
<fx:root type="AnchorPane" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="cn.kizzzy.javafx.control.LabeledChooser">
    <children>
        <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                   AnchorPane.topAnchor="0.0">
            <children>
                <LabeledTextField fx:id="ltf" content="">
                </LabeledTextField>
                <Button fx:id="button" contentDisplay="GRAPHIC_ONLY" StackPane.alignment="CENTER_RIGHT">
                    <StackPane.margin>
                        <Insets right="8.0"/>
                    </StackPane.margin>
                </Button>
            </children>
        </StackPane>
    </children>
    <stylesheets>
        <URL value="@../../css/button.css"/>
    </stylesheets>
</fx:root>
